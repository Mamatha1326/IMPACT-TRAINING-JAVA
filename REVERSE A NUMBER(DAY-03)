
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num=sc.nextInt();
        int first=(num/100);
        int second=((num/10)%10);
        int third=(num%10);
        int sum=(third*100)+(second*10)+(first*1);
        

        System.out.println(sum);
    }
}
