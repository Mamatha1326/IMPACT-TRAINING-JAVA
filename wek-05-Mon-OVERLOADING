// write a prg using  method overrloading to find the sum of 2 numbers
import java.util.*;
class Sum{
    int add(int a,int b){
        return a+b;
    }
    double add (double a, double b){
        return a+b;
    }
    
}
class Main {
    public static void main(String[]args){
        Sum sum=new Sum();
        System.out.println(sum.add(5.5,10.5));
        System.out.println(sum.add(5,10));
    }
}
