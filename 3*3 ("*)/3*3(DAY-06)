
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num=sc.nextInt();
        for(int i=0;i<=2;i++){
            for(int j=0;j<=2;j++){
                System.out.print("* \t ");
            }
            System.out.println("");
        }
      

       
    }
}
