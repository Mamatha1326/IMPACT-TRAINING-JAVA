
import java.util.*;
public class Main{
    public static void main(String[]args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter a nummber:");
        int n=sc.nextInt();
        System.out.println("even numbers from 1 to " + n + ":");
        for (int i=1;i<n;i++){
            if(i%2==0){
                System.out.print(i+" ");
            }
        }

        sc.close();
        
    }
}
